<column index="1" value="54"/><column index="2" value="58"/><column index="3" value="84"/><column index="4" value="61"/><column index="5" value="49"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 10">-- 
-- Run Query: Find the jobs done by a with 12 or more tracks.
-- 

SELECT o.customer_id
FROM orders o
JOIN customers c ON o.customer_id = c.customer_id
GROUP BY o.customer_id
HAVING COUNT(o.customer_id) &gt;= 12;</sql><current_tab id="0"/></tab_sql></sqlb_project>
